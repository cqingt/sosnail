{
  "name": "sosnail",
  "version": "0.0.1",
  "keywords": [
    "sosnail",
    "random",
    "utils",
    "generator"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon --watch webpack.config.js --exec webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 1,
      "eqeqeq": 1
    }
  },
  "dependencies": {
    "cross-env": "^5.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.16",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.12.0",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.1"
  }
}
